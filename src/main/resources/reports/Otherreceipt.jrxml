	<queryString>
		<![CDATA[SELECT IFNULL(name, `Payee/Payer`) as name,
													 `Ref #`,CONCAT(b.`Account Name`,' ',b.`Account #`) AS ACCOUNT,a.Description,
													 SUM(credit) AS amount,date from accounttransactions a
													 LEFT JOIN members r ON `Payee/Payer`=`id`  
													 INNER JOIN bankaccounts b ON b.`Acc id`=a.bank 
													where `Transaction id`=1]]>
	</queryString>
