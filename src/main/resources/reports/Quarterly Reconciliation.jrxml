	<queryString language="SQL">
		<![CDATA[SELECT 

sum( if( ACCOUNT=1 and DATE BETWEEN 1 AND '2019-12-31' ,(credit-debit), 0 ) ) AS openingbalance,

sum( if( ACCOUNT=1 and DATE BETWEEN '2020-01-01' AND '2020-03-31' AND description!='Cash Deposit' ,(credit), 0 ) ) AS march,
sum( if( ACCOUNT=1 and DATE BETWEEN '2020-04-01' AND '2020-06-31' AND description!='Cash Deposit' ,(credit), 0 ) ) AS june,
sum( if( ACCOUNT=1 and DATE BETWEEN '2020-07-01' AND '2020-09-31' AND description!='Cash Deposit' ,(credit), 0 ) ) AS september,
sum( if( ACCOUNT=1 and DATE BETWEEN '2020-10-01' AND '2020-12-31' AND description!='Cash Deposit' ,(credit), 0 ) ) AS december,


sum( if( ACCOUNT=1 and Month(DATE) ='01' AND SUBSTRING(`ref #`,1,2)='PV' ,(debit), 0 ) ) AS jan,
sum( if( ACCOUNT=1 and Month(DATE) ='02' AND SUBSTRING(`ref #`,1,2)='PV' ,(debit), 0 ) ) AS feb,
sum( if( ACCOUNT=1 and Month(DATE) ='03' AND SUBSTRING(`ref #`,1,2)='PV' ,(debit), 0 ) ) AS mar,
sum( if( ACCOUNT=1 and Month(DATE) ='04' AND SUBSTRING(`ref #`,1,2)='PV' ,(debit), 0 ) ) AS apr,
sum( if( ACCOUNT=1 and Month(DATE) ='05' AND SUBSTRING(`ref #`,1,2)='PV' ,(debit), 0 ) ) AS may,
sum( if( ACCOUNT=1 and Month(DATE) ='06' AND SUBSTRING(`ref #`,1,2)='PV' ,(debit), 0 ) ) AS jun,
sum( if( ACCOUNT=1 and Month(DATE) ='07' AND SUBSTRING(`ref #`,1,2)='PV' ,(debit), 0 ) ) AS jul,
sum( if( ACCOUNT=1 and Month(DATE) ='08' AND SUBSTRING(`ref #`,1,2)='PV' ,(debit), 0 ) ) AS aug,
sum( if( ACCOUNT=1 and Month(DATE) ='09' AND SUBSTRING(`ref #`,1,2)='PV' ,(debit), 0 ) ) AS sep,
sum( if( ACCOUNT=1 and Month(DATE) ='10' AND SUBSTRING(`ref #`,1,2)='PV' ,(debit), 0 ) ) AS octt,
sum( if( ACCOUNT=1 and Month(DATE) ='11' AND SUBSTRING(`ref #`,1,2)='PV' ,(debit), 0 ) ) AS nov,
sum( if( ACCOUNT=1 and Month(DATE) ='12' AND SUBSTRING(`ref #`,1,2)='PV' ,(debit), 0 ) ) AS dece,
sum( if( ACCOUNT=1 and year(DATE) ='2020' AND SUBSTRING(`ref #`,1,2)='PV' ,(debit), 0 ) ) AS totalexpe


 FROM accounttransactions]]>
	</queryString>
